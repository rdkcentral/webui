//bit-and limited to 32, I have to write this
function php_str_and($a, $b)
{
	var $c = "";
	for (var $i=0; $i<16; $i++)
	{
		$c = $c+dechex((hexdec(substr($a,$i,1)) & hexdec(substr($b,$i,1))));
	}
